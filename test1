    var a = 10;


    for( var i = 1, i <= a ; i++ ){

        var detect = 0;

        var FIZZ = ( i%3 == 0 ) ? detect = 'F' :  ;
        var BUZZ = ( i%5 == 0 ) ? detect = 'B' :  ;
        var BOTH = ( i%3 == 0 && i%5 == 0 ) ? detect = 'BOTH' :  ;

        switch( detect ){
          case 'BOTH' :
          console.log('FIZZ BUZZ');
          break;
          case 'F' :
          console.log('FIZZ');
          break;
          case 'B' :
          console.log('BUZZ');
          break;
          default :
          console.log( i );
          break;
        }


